{
     "name": "nest-typescript-starter",
     "version": "1.0.0",
     "description": "Nest TypeScript repository",
     "license": "MIT",
     "scripts": {
          "prebuild": "rimraf dist",
          "build": "nest build",
          "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
          "start": "nest start --watch --exec 'node --env-file local.env dist/backend/src/main'",
          "start:debug": "nest start --debug --watch --exec 'node --env-file local.env dist/backend/src/main'",
          "start:prod": "node --env-file prod.env dist/backend/src/main",
          "lint": "eslint '{src,apps,libs,test}/**/*.ts'",
          "lint:fix": "npm run lint -- --fix",
          "test": "mocha",
          "test:watch": "jest --watch",
          "test:cov": "jest --coverage",
          "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
          "test:e2e": "echo 'No e2e tests implemented yet.'"
     },
     "scriptComments": {
          "start": "Nearly the same as 'nest build && node --watch-path=./src dist/backend/src/main' "
     },
     "dependencies": {
          "@nestjs/common": "11.0.6",
          "@nestjs/core": "11.0.6",
          "@nestjs/jwt": "^11.0.0",
          "@nestjs/platform-express": "11.0.6",
          "@nestjs/platform-socket.io": "^11.0.7",
          "@nestjs/serve-static": "5.0.1",
          "@nestjs/typeorm": "^11.0.0",
          "@nestjs/websockets": "^11.0.7",
          "class-transformer": "0.5.1",
          "class-validator": "0.14.1",
          "pg": "^8.14.1",
          "reflect-metadata": "0.2.2",
          "rimraf": "6.0.1",
          "rxjs": "7.8.1",
          "typeorm": "^0.3.21"
     },
     "devDependencies": {
          "@eslint/eslintrc": "3.2.0",
          "@eslint/js": "9.19.0",
          "@nestjs/cli": "11.0.2",
          "@nestjs/schematics": "11.0.0",
          "@nestjs/testing": "11.0.6",
          "@types/express": "5.0.0",
          "@types/mocha": "^10.0.10",
          "@types/node": "^22.12.0",
          "@types/supertest": "6.0.2",
          "earl": "^1.3.0",
          "eslint": "9.19.0",
          "eslint-config-prettier": "^10.1.1",
          "eslint-plugin-import-typescript": "^0.0.4",
          "eslint-plugin-prettier": "5.2.3",
          "globals": "15.14.0",
          "jest": "29.7.0",
          "mocha": "^11.1.0",
          "prettier": "3.4.2",
          "supertest": "7.0.0",
          "ts-jest": "29.2.5",
          "ts-loader": "9.5.2",
          "ts-node": "^10.9.2",
          "tsconfig-paths": "4.2.0",
          "typescript": "5.7.3",
          "typescript-eslint": "8.22.0"
     }
}
